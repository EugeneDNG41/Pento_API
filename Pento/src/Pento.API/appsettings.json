{
  "ConnectionStrings": {
    "pento-db": "",
    "Redis": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Pento.Api"
    }
  },
  "Authentication": {
    "Audience": "",
    "ValidIssuer": "",
    "MetadataUrl": "",
    "RequireHttpsMetadata": false
  },
  "KeyCloak": {
    "AdminUrl": "",
    "TokenUrl": "",
    "ConfidentialClientId": "",
    "ConfidentialClientSecret": "",
    "PublicClientId": ""
  },
  "Outbox": {
    "IntervalInSeconds": 10,
    "BatchSize": 10
  },
  "AllowedHosts": "*"
}
