{
  "ConnectionStrings": {
    "pento-db": "",
    "Redis": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },

    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://seq:5431" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Pento.Api"
    }
  },
  "Authentication": {
    "Audience": "",
    "ValidIssuer": "",
    "MetadataUrl": "",
    "RequireHttpsMetadata": false
  },
  "KeyCloak": {
    "AdminUrl": "",
    "TokenUrl": "",
    "ConfidentialClientId": "",
    "ConfidentialClientSecret": "",
    "PublicClientId": ""
  },
  "PayOS": {
    "ClientId": "",
    "ApiKey": "",
    "ChecksumKey": "",
    "WebhookUrl": "",
    "ReturnUrl": "",
    "CancelUrl": ""
  },
  "Outbox": {
    "IntervalInSeconds": 10,
    "BatchSize": 10
  },
  "Google": {
    "Type": "",
    "ProjectId": "",
    "PrivateKeyId": "",
    "PrivateKey": "",
    "ClientEmail": "",
    "ClientId": "",
    "AuthUri": "",
    "TokenUri": "",
    "AuthProviderX509CertUrl": "",
    "ClientX509CertUrl": "",
    "UniverseDomain": ""
  },
  "AllowedOrigins": "https://pento-food.vercel.app"
}
